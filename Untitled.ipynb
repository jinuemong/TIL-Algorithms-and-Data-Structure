    "T = int(input())\n",
    "a_sum = [0]*T\n",
    "for i in range(T):\n",
    "    a = list(map(int, input().split()))\n",
    "    for item in a:\n",
    "        if item % 2 == 1:\n",
    "            a_sum[i] += item\n",
    "for i in range(T):\n",
    "    print(\"#\" + str(i + 1), a_sum[i])"
   ]
